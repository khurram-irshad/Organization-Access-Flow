<h1>Analytics for <%= @organization.name %></h1>
<p>Total Users: <%= @user_count %></p>
<h2>Role Distribution</h2>
<ul>
<% @role_counts.each do |role, count| %>
  <li><%= role %>: <%= count %></li>
<% end %>
</ul>
<h2>Content Distribution by Rating</h2>
<ul>
<% @content_counts.each do |rating, count| %>
  <li><%= rating %>: <%= count %></li>
<% end %>
</ul>