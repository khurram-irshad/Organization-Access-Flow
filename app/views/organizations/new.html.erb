<h1>New Organization</h1>
<%= form_with model: @organization, local: true do |f| %>
  <div>
    <%= f.label :name %>
    <%= f.text_field :name, required: true %>
  </div>
  <div>
    <%= f.label :description %>
    <%= f.text_area :description %>
  </div>
  <%= f.submit "Create Organization" %>
<% end %>
<%= link_to "Back", organizations_path %>